{
  "name": "@kjanat/prettier-config",
  "version": "1.1.0",
  "type": "module",
  "description": "Yet another Prettier config",
  "repository": "github:kjanat/prettier-config",
  "homepage": "https://github.com/kjanat/prettier-config#readme",
  "bugs": {
    "url": "https://github.com/kjanat/prettier-config/issues"
  },
  "author": "Kaj Kowalski <dev@kajkowalski.nl> (https://github.com/kjanat)",
  "license": "MIT",
  "packageManager": "bun@1.3.2",
  "main": "./build/prettier.config.js",
  "types": "./build/prettier.config.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./build/prettier.config.d.ts",
        "default": "./build/prettier.config.js"
      },
      "require": {
        "types": "./build/prettier.config.d.cts",
        "default": "./build/prettier.config.cjs"
      }
    }
  },
  "files": [
    "build/",
    "prettier.config.ts",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "prettier-config"
  ],
  "scripts": {
    "build": "bun run build:cjs",
    "build:cjs": "bun build.ts --cjs",
    "build:esm-only": "bun build.ts",
    "format": "prettier . --write",
    "prepare": "bun run build",
    "prepublishOnly": "bun typecheck && bun run build",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@prettier/plugin-xml": "^3.4.2",
    "prettier-plugin-go-template": "^0.0.15",
    "prettier-plugin-nginx": "^1.0.3",
    "prettier-plugin-pkg": "^0.21.2",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-sh": "^0.18.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prettier-plugin-toml": "^2.0.6"
  },
  "devDependencies": {
    "@types/bun": "^1.3.2",
    "bun-osv-scanner": "^2.0.0",
    "lefthook": "^2.0.4",
    "prettier": "^3.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}

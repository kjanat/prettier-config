{
  "name": "@kjanat/prettier-config",
  "version": "1.0.0",
  "description": "My personal Prettier config",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "index.ts",

{
  "name": "@kjanat/prettier-config",
  "version": "1.1.0",
  "description": "Yet another Prettier config",
  "keywords": [
    "prettier",
    "prettier-config",
    "tailwindcss",
    "xml"
  ],
  "homepage": "https://github.com/kjanat/prettier-config#readme",
  "bugs": {
    "url": "https://github.com/kjanat/prettier-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjanat/prettier-config.git"
  },
  "license": "MIT",
  "author": "kjanat",
  "type": "module",
  "exports": "./build/prettier.config.js",
  "files": [
    "build/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "bun build.ts",

  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier . --write",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "prettier-plugin-packagejson": "^2.5.19"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^3.4.2",
    "@types/bun": "latest",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
